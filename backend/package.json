{
    "name": "deployment-backend",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "start": "node .",
        "dev": "nodemon src/app.ts --watch",
        "clean": "rmdir /Q /S www",
        "copy:scripts": "@powershell copy ./package.json ./www/package.json && @powershell copy ./.npmrc ./www/.npmrc",
        "deploy": "chmod +x ./bin/deploy.sh && ./bin/deploy.sh",
        "build": "rm -r ./www && npx tsc && cd www && cp ../package.json ./ && zip -r app.zip ./* && ../",
        "db:up": "db-migrate up",
        "db:down": "db-migrate reset",
        "test": "echo \"Error: no test specified\""
    },
    "keywords": [
        "nodejs",
        "express",
        "postgres"
    ],
    "author": "Mahmoud Kassem",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "morgan": "^1.10.0",
        "pg": "^8.7.3",
        "prettier": "^2.5.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/morgan": "^1.9.3",
        "@types/pg": "^8.6.4",
        "nodemon": "^2.0.15",
        "ts-node": "^10.9.1",
        "typescript": "^4.5.5"
    }
}